<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified">
	<xsd:element name="ArrayOfCarInfo">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element maxOccurs="unbounded" name="CarInfo">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Model" type="xsd:string" />
							<xsd:element name="Brand" type="xsd:string" />
							<xsd:element name="Price" type="xsd:unsignedShort" />
							<xsd:element name="FuelConsumption" type="xsd:decimal" />
							<xsd:element name="FuelType" type="xsd:string" />
							<xsd:element name="Reliability" type="xsd:unsignedByte" />
							<xsd:element name="ComfortLevel" type="xsd:string" />
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xs:schema>